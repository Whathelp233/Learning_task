<launch>

	<param name="robot_description" textfile="../urdf/robot.urdf"/>
	<rosparam file="../config/config.yaml" command="load"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
    </include>
	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" clear_params="true"
          args="-z 0.5 -param robot_description -urdf -model car" output="screen"/>
	<node name="controller_spawner" pkg="controller_manager" type="spawner" output="screen"
	 args="/controller/joint_state_controller
            /controller/diff_drive_controller"/>
	<node name="rviz" pkg="rviz" type="rviz" args="-d ../config/urdf.rviz"/>

</launch>
